{
  "name": "@uiw/reset.css",
  "version": "1.0.4",
  "description": "A tiny modern CSS reset.",
  "main": "reset.css",
  "files": [
    "reset.css",
    "reset.less",
    "reset.min.css"
  ],
  "keywords": [
    "css",
    "less",
    "reset",
    "mini"
  ],
  "repository": "https://github.com/uiwjs/reset-css",
  "scripts": {
    "start": "npm run compile && npm run compile:min",
    "prepare": "npm run start",
    "compile": "lessc reset.less | bannerjs -m > reset.css",
    "compile:min": "lessc reset.less | bannerjs -o | csso > reset.min.css"
  },
  "devDependencies": {
    "bannerjs": "1.0.6",
    "csso-cli": "^2.0.2",
    "less": "3.9.0"
  },
  "license": "MIT"
}
